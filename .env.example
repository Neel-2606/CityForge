# NASA API Keys (REAL CREDENTIALS - Replace with your actual values)
# Option 1: Username/Password
NASA_EARTHDATA_USERNAME=your_real_username
NASA_EARTHDATA_PASSWORD=your_real_password

# Option 2: Access Token (preferred for better access)
NASA_EARTHDATA_TOKEN=your_access_token_here

# Google Earth Engine
GOOGLE_EARTH_ENGINE_SERVICE_ACCOUNT_KEY=path/to/your-real-service-account.json

# Supabase Database Configuration (Replace PostgreSQL)
SUPABASE_URL=https://your-project-id.supabase.co
SUPABASE_ANON_KEY=your_anon_key
SUPABASE_SERVICE_KEY=your_service_role_key
DATABASE_URL=postgresql://postgres:your_password@db.your-project-id.supabase.co:5432/postgres

# Legacy PostgreSQL (Keep for fallback)
POSTGRES_USER=postgres
POSTGRES_PASSWORD=your_password
POSTGRES_DB=urban_resilience

# API Configuration
API_HOST=0.0.0.0
API_PORT=8000
DEBUG=True

# External APIs
WORLDPOP_API_KEY=your_worldpop_key
COPERNICUS_API_KEY=your_copernicus_key

# Storage
AWS_ACCESS_KEY_ID=your_aws_key
AWS_SECRET_ACCESS_KEY=your_aws_secret
AWS_BUCKET_NAME=urban-resilience-data
AWS_REGION=us-east-1
